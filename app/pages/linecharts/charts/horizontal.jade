chart(data="chartData", tooltips)
   layout(line, markers="{{options.markers}}")
      x(property="date", type="time", axis, date-format="%Y%m%d", label="Date", show-label)
      y(axis, show-label, label="Temperature (F)", grid, legend="bottom-right", legend-title="City")
      color(type="category10", legend-title="City" )


